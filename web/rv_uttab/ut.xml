<krpano version="1.16" title="Virtual Tour" onstart="loadscene(scene_08, null, MERGE);">
<autorotate enabled="true" 
            waittime="5.0" 
            accel="1.2" 
            speed="2.5" 
            horizon="off"
            />

	<!-- disable the default progress bar -->
	<progress showload="none" showwait="none" />
	
	<plugin name="gyro" devices="ios"
        url="plugins/gyro.js" 
        keep="true"
        enabled="true"
        camroll="false"
        friction="0.5"
        velastic="0"
        vrelative="true"
        onavailable=""
        />

<!-- botones (zoom, direction) para html5  -->
<plugin name="in"     
url="skin/btn_plus.png"  
devices="html5" 
keep="true"
align="bottom" x="-125" y="30"  
ondown="set(fov_moveforce,-1);"  
onup="set(fov_moveforce,0);" />

<plugin name="out"      
url="skin/btn_minus.png"
devices="html5" 
keep="true"
align="bottom" x="-75" y="30"  
ondown="set(fov_moveforce,+1);"  
onup="set(fov_moveforce,0);" />

<plugin name="left"   
url="skin/btn_left.png"  
devices="html5" 
keep="true"
align="bottom" x="-25"  y="30"  
ondown="set(hlookat_moveforce,-1);"  
onup="set(hlookat_moveforce,0);" />

<plugin name="right"  
url="skin/btn_right.png" 
devices="html5" 
keep="true"
align="bottom" x="25"  y="30" 
ondown="set(hlookat_moveforce,+1);"  
onup="set(hlookat_moveforce,0);" />

<plugin name="up"   
url="skin/btn_up.png"    
devices="html5" 
keep="true"
align="bottom" x="75"   y="30" 
ondown="set(vlookat_moveforce,-1);"  
onup="set(vlookat_moveforce,0);" />

<plugin name="down"   
url="skin/btn_down.png"  
devices="html5" 
keep="true"
align="bottom" x="125"  y="30" ondown="set(vlookat_moveforce,+1);"  onup="set(vlookat_moveforce,0);" />

<!-- botones (zoom, direction) para flash   -->

<plugin name="in2"    url="skin/btn_plus.png"  
devices="flash" 
keep="true"
align="bottom" y="30" x="-120" blendmode="normal" 
ondown="set(movevectorz,-1);"  onup="set(movevectorz,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" />

<plugin name="out2"     
url="skin/btn_minus.png"  
devices="flash" 
keep="true"
align="bottom" x="-80" y="30"  
ondown="set(movevectorz,+1);"  onup="set(movevectorz,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" />

<plugin name="left2"     
url="skin/btn_left.png"  
devices="flash" 
keep="true"
align="bottom" x="-40" y="30"  
ondown="set(movevectorx,-1);"  onup="set(movevectorx,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" />

<plugin name="right2"     
url="skin/btn_right.png"  
devices="flash" 
keep="true"
align="bottom" x="0" y="30"  
ondown="set(movevectorx,+1);"  onup="set(movevectorx,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" />

<plugin name="up2"    url="skin/btn_up.png"    
devices="flash"
keep="true" 
align="bottom"  y="30" x="+40"   blendmode="normal" ondown="set(movevectory,-1);"  onup="set(movevectory,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" />

<plugin name="down2"  url="skin/btn_down.png"  
devices="flash" 
keep="true"
align="bottom"  y="30" x="+80"  blendmode="normal" ondown="set(movevectory,+1);"  onup="set(movevectory,0);" onover="set(blendmode,add);" onout="set(blendmode,normal);" /> 

<plugin name="openfs"  url="skin/btn_openfs.png"  
devices="flash"
keep="true" 
visible="true"  align="bottom"  y="30" x="+120"  blendmode="normal" 
onover="set(blendmode,add);"  onout="set(blendmode,normal);" onclick="set(fullscreen,true);"   />

<plugin name="closefs" url="skin/btn_closefs.png" 
devices="flash"
keep="true" 
visible="false" align="bottom"  y="30" x="+120"  blendmode="normal" 
onover="set(blendmode,add);"  onout="set(blendmode,normal);" onclick="set(fullscreen,false);"  />

<events 
onenterfullscreen="set(plugin[openfs].visible,false); 
set(plugin[closefs].visible,true);"
onexitfullscreen="set(plugin[openfs].visible,true); 
set(plugin[closefs].visible,false);"
/>

<plugin name="logo" 
url="skin/logo.png" 
keep="true"
blendmode="normal" 
align="rightbottom"
x="5" y="5"
enabled="false"
onclick="openurl(http://www.recorridosvirtuales.com,blank);" />

<!-- gyro -->
<plugin name="gyro"
	devices="plugins/ios"
	url="gyro.js"
	keep="true"
	enabled="true"
	camroll="false"
	friction="0.5"
	velastic="0"
/>

