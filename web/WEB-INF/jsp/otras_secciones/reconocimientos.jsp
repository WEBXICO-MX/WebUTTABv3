        <script>
            $(function () {
                $("#medallero01_dialog").dialog({
                    autoOpen: false,
                    width: 520,
                    height: 600,
                    show: {
                        effect: "blind",
                        duration: 1000
                    },
                    hide: {
                        effect: "explode",
                        duration: 1000
                    }
                });

            });

            $(".otras_secciones_texto img").on("click", function () {
                var alt = this.alt;
                var src = this.src.substring(this.src.lastIndexOf("/") + 1, this.src.length);
                src = src.substring(0, 9) + "-GRANDE" + src.substring(9, 12) + "-min.png";
                modal(alt, src);

            });

            function modal(xTitulo, xIMG)
            {
                $('#medallero01_dialog').load('reconocimientos-modal.action', {"xTitulo": xTitulo, "xIMG": xIMG}, function () {
                    $("span.ui-dialog-title").text(xTitulo);
                }).dialog('open');

