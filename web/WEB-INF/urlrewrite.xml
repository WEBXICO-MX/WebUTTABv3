<urlrewrite>
    <rule>
        <name>seo redirect</name>
        <condition name="host" operator="notequal">^uttab.edu.mx</condition>
        <condition name="host" operator="notequal">^www.uttab.edu.mx</condition>
        <condition name="host" operator="notequal">^localhost</condition>
        <condition name="host" operator="notequal">^192.168.2.4</condition>
        <from>^/(.*)</from>
        <to type="permanent-redirect" last="true">http://www.uttab.edu.mx/</to>
    </rule>
    <!-- 60 * 60 * 24 * 7 (60 segundos x 60 minutos x 24 horas x 7 dÃ­as)-->
    <rule>
        <from>^.*\.(js|css|gif|jpg|jpeg|png|pdf)$</from>
        <set type="response-header" name="Cache-Control">max-age=604800, must-revalidate</set>
    </rule>
</urlrewrite>
